<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="customerSelectDao">
	<select id="selectCustomer" parameterType="customerSelectVO" resultType="customerSelectVO">
		SELECT customerid, customerkana, customername1, customername2, state, post,
		address1, address2, address3, telephone1, telephone2, fax, email FROM "customerinfo"
	</select>
</mapper>